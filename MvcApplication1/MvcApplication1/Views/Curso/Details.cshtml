@model MvcApplication1.Models.CursoDB

@{
    ViewBag.Title = "Details";
}

<h2>Detalhes do curso</h2>
<div class="panel panel-default">
    <div class="panel-heading">
        <h2 class="panel-title">curso</h2>
    </div>
    <div class="acoes panel body">
        <table class="table">
            <tr>
                <th>@Html.DisplayNameFor(model => model.cursoID)</th>
                <th>@Html.DisplayNameFor(model => model.nome)</th>
                <th>@Html.DisplayNameFor(model => model.universidadeNome)</th>
            </tr>
            <tr>
                <td>@Html.DisplayFor(model => model.cursoID)</td>
                <td>@Html.DisplayFor(model => model.nome)</td>
                <td>@Html.DisplayFor(model => model.universidadeNome)</td>
            </tr>

        </table>
    </div>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.cursoID }, new { @class = "btn btn-primary" }) |
    @Html.ActionLink("Voltar", "Index", null, new { @class = "btn btn-default" })
</p>
