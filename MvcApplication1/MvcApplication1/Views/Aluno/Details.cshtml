@model IEnumerable<MvcApplication1.Models.LogModel>

@{
    ViewBag.Title = "Log";
}
@if (Model != null)
{
    <table class="table">
        <thead>
            <tr>
                <th>#</th>
                <th>Alterado por</th>
                <th>Alterado em</th>
                <th>Nome</th>
                <th>Data de Nascimento</th>
                <th>Email</th>
                <th>Curso</th>
                <th>Universidade</th>
            </tr>
        </thead>

        <tbody>
            @{int i = 1;}
            @foreach (var item in Model)
            {
                <tr>
                    <td>@i</td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Autor)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Horario)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Aluno)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.AlunoNascimento)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.AlunoEmail)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Curso)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Universidade)
                    </td>
                </tr>
                i++;
            }
        </tbody>
    </table>
}


